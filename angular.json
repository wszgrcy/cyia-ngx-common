{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "cyia-ngx-common": {
      "root": "",
      "sourceRoot": "demo/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./webpack.config.ts"
            },
            "allowedCommonJsDependencies": [
              "monaco-editor"
            ],
            "aot": true,
            "outputPath": "dist/cyia-ngx-common-demo",
            "index": "demo/src/index.html",
            "main": "demo/src/main.ts",
            "polyfills": "demo/src/polyfills.ts",
            "tsConfig": "demo/src/tsconfig.app.json",
            "assets": [
              "demo/src/favicon.ico",
              "demo/src/assets",
              {
                "glob": "*.wasm",
                "input": "node_modules/vscode-oniguruma/release",
                "output": "assets/vscode-oniguruma"
              }
            ],
            "styles": [
              "demo/src/styles.css",
              "./node_modules/monaco-editor/min/vs/editor/editor.main.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "demo/src/environments/environment.ts",
                  "with": "demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "cyia-ngx-common:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "cyia-ngx-common:build:production",
              "disableHostCheck": true
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cyia-ngx-common:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "demo/src/test.ts",
            "polyfills": "demo/src/polyfills.ts",
            "tsConfig": "demo/src/tsconfig.spec.json",
            "karmaConfig": "demo/src/karma.conf.js",
            "styles": [
              "demo/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "demo/src/favicon.ico",
              "demo/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "cyia-ngx-common-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "cyia-ngx-common:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "cyia-ngx-common:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "lib": {
      "root": "lib",
      "sourceRoot": "lib/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "lib/tsconfig.lib.json",
            "project": "lib/src/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "lib/src/ng-package.prod.json",
              "tsConfig": "lib/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "lib/src/test.ts",
            "tsConfig": "lib/tsconfig.spec.json",
            "karmaConfig": "lib/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "fix": true,
            "tsConfig": [
              "lib/tsconfig.lib.json",
              "lib/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "cyia-ngx-common",
  "cli": {
    "analytics": false
  }
}