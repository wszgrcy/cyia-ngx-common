{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "lib": {
      "root": "lib",
      "sourceRoot": "lib/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "tsConfig": "lib/tsconfig.lib.json",
            "project": "lib/src/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "lib/src/ng-package.prod.json",
              "tsConfig": "lib/tsconfig.lib.prod.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "sourceMap": true,
            "tsConfig": "lib/tsconfig.spec.json"
          }
        }
      }
    },
    "examples": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "examples",
      "sourceRoot": "examples/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/examples"
            },
            "index": "examples/src/index.html",
            "polyfills": ["zone.js"],
            "tsConfig": "examples/tsconfig.app.json",
            "aot": true,
            "assets": [
              "examples/src/favicon.ico",
              "examples/src/assets",
              {
                "glob": "*.wasm",
                "input": "node_modules/vscode-oniguruma/release",
                "output": "assets/vscode-oniguruma"
              }
            ],
            "styles": ["examples/src/styles.scss", "./node_modules/monaco-editor/min/vs/editor/editor.main.css"],
            "scripts": [],
            "browser": "examples/src/main.ts"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "examples/src/environments/environment.ts",
                  "with": "examples/src/environments/environment.prod.ts"
                }
              ],
              "deployUrl": "assets/examples/scripts/",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "examples:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "examples:build:production"
            },
            "development": {
              "buildTarget": "examples:build:development"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "examples:build"
          }
        },

        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["examples/tsconfig.app.json", "examples/tsconfig.spec.json", "examples/e2e/tsconfig.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
